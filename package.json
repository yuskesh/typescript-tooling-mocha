{
  "name": "typescript-tooling-mocha",
  "version": "1.0.0",
  "description": "Basic TypeScript Tooling Set using mocha/nyt",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "cover": "nyc npm t",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Yusuke Shinozaki",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "sourceMap": true,
    "all": true
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.15",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
